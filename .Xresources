!General Things
URxvt.tabbed.no-tabbedex-keys: true
urxvt*shell:/usr/bin/zsh
urxvt*loginShell:true
!urxvt*inheritPixmap: true
urxvt*tintColor: #5e2750
urxvt*transparent:true
urxvt*shading: 100
urxvt*fading: 30
urxvt*scrollBar:false
!urxvt*transpscrollbar:true
urxvt*saveLines:50000

! Font for Xterm
! Font
URxvt*letterSpace: 4

XTerm.vt100.faceName: DejaVu Sans Mono:size=12:antialias=true
